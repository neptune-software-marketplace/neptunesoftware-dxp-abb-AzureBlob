		"==OBJECT STRING==",
		"const azureblob = globals.List;",
		"",
		"const blobData = {",
		"    \"account\": \"\", //please add here your Azure blob account name",
		"    \"container\": \"\", //please add here your Azure blob container name",
		"    \"accountKey\": \"\", //please add here your Azure blob access account key ",
